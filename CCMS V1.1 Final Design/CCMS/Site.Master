                                    <li>
                                        <asp:LinkButton ID="lnkAddStudent" runat="server" Visible="false"></asp:LinkButton>
                            
                                    </li>

                                    <li>
                                        <asp:LinkButton ID="lnkEditStudent" runat="server" Visible="false"></asp:LinkButton>
                                    </li>
                                </ul>
                            </li>
