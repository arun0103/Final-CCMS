                </asp:ContentPlaceHolder>
                </asp:Panel>

                <asp:Panel ID ="menubaruser" runat="server" Visible="false">
                    <asp:ContentPlaceHolder ID="menuuser" runat="server">
                                <div id="cssmenuUser">
                                    <ul>

